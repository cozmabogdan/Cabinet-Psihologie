<!--header-->
<%- include("partials/header"); -%>

<% posts.forEach(function(post){%>
    <h2 class="mt-4"> <%= post.title %> </h2>
    <p class="text-dark">
        <%=post.content.substring(0, 200) + " ..." %>
        <a class="text-primary" href="/posts/<%=post._id%>">Citeste mai mult</a>
    </p>
<%})%>

<!--footer-->
<%- include("partials/footer"); -%>