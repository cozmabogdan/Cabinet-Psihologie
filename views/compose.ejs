<!--header-->
<%- include("partials/header") -%>

<div class="row justify-content-center">
    <div class="col-md-10">
        <hr class="my-5">
        <div class="card card-outline-secondary">
            <div class="card-header">
                <h3 class="mb-0" style="text-align: center;">Dashboard</h3>
            </div>
            <div class="card-body">
                <form autocomplete="off" class="form" role="form">
                    <fieldset>
                        <label class="mb-0" for="title">Titlu</label>
                        <div class="row mb-2">
                            <div class="col-lg-12">
                                <input class="form-control" id="name" name="name" required="" type="text">
                            </div>
                        </div>
                        <label class="mb-0" for="phone">Telefon</label>
                        <div class="row mb-2">
                            <div class="col-lg-12">
                                <input class="form-control" id="phone" name="phone" required="" type="text">
                            </div>
                        </div>
                        <label class="mb-0" for="email">Email</label>
                        <div class="row mb-2">
                            <div class="col-lg-12">
                                <input class="form-control" id="email" name="email" required="" type="text">
                            </div>
                        </div>
                        <label class="mb-0" for="content">Articol</label>
                        <div class="row mb-2">
                            <div class="col-lg-12">
                                <textarea class="form-control" id="message" name="message" required="" rows="6"></textarea>
                            </div>
                        </div>
                        <button class="btn btn-secondary btn-lg float-right mt-2" type="submit">Posteaza</button>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>

<!--footer-->
<%- include("partials/footer") -%>